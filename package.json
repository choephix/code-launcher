{
  "name": "code-launcher",
  "version": "1.0.41",
  "description": "",
  "author": "Stefan Ginev",
  "license": "GPL-3.0",
  "main": "build/index.js",
  "homepage": "http://github.com/choephix/code-launcher",
  "bugs": "https://github.com/choephix/code-launcher/issues",
  "bin": {
    "code-launcher": "./build/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/choephix/code-launcher.git"
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "start": "pnpm run build && node ./build/server/index.js",
    "build": "pnpm --filter @code-launcher/client-with-vite run build && pnpm --filter @code-launcher/server-with-fastify run build",
    "dev": "pnpm -r --filter @code-launcher/client-with-vite --filter @code-launcher/server-with-fastify run dev",
    "prepublishOnly": "pnpm run build && npm version patch --force",
    "update:all": "pnpm update --recursive --latest"
  },
  "keywords": [],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
